    {
      "parameters": {
        "operation": "executeQuery",
        "query": "INSERT INTO usuarios_quest (\n  usuario_id,\n  catalogo_id,\n  status,\n  config,\n  ativado_em\n)\nVALUES (\n  $1::uuid,\n  '00000000-0000-0000-0000-000000000001',\n  'disponivel',\n  $2::jsonb,\n  NOW()\n)\nRETURNING id, catalogo_id, status, config;",
        "options": {
          "queryReplacement": "={{ [\n  $json.usuario_id,\n  JSON.stringify({\n    titulo: $json.pedido.substring(0, 80),\n    descricao: $json.pedido,\n    contexto_origem: 'pedido_explicito',\n    prioridade: 'alta'\n  })\n] }}"
        }
      },
