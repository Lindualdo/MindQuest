      ]
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "-- Upsert log + finaliza sessão em uma transação\nWITH upsert_log AS (\n  INSERT INTO log_experts (usuario_id, chat_id, expert_name, ultima_interaction, historico, processado_em, atualizado_em)\n  VALUES (\n    $1::uuid,\n    $2::uuid,\n    'hub',\n    $3::int,\n    jsonb_build_array(jsonb_build_object('de', 0, 'ate', $3::int, 'novas', $3::int, 'em', NOW())),\n    NOW(),\n    NOW()\n  )\n  ON CONFLICT (chat_id, expert_name) DO UPDATE SET\n    historico = log_experts.historico || jsonb_build_array(jsonb_build_object(\n      'de', log_experts.ultima_interaction,\n      'ate', $3::int,\n      'novas', $3::int - log_experts.ultima_interaction,\n      'em', NOW()\n    )),\n    ultima_interaction = $3::int,\n    atualizado_em = NOW()\n  RETURNING chat_id\n)\nUPDATE usr_chat SET\n  status = 'finalizado',\n  horario_fim = NOW(),\n  atualizado_em = NOW()\nWHERE id = $2::uuid\nRETURNING id, usuario_id, status;",
        "options": {
          "queryReplacement": "={{ [$json.usuario_id, $json.chat_id, $json.total_interactions] }}"
        }
      },
