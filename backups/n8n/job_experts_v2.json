    {
      "parameters": {
        "jsCode": "// Prepara dados e valida delta de palavras\nconst item = $input.first().json;\nconst mensagens = item.mensagens || [];\nconst ultimaProcessada = item.ultima_processada || 0;\n\n// Extrai apenas mensagens novas (delta)\nconst mensagensNovas = Array.isArray(mensagens) ? mensagens.slice(ultimaProcessada) : [];\n\n// Se não há mensagens novas, skip\nif (mensagensNovas.length === 0) {\n  return [{\n    json: {\n      skip: true,\n      motivo: 'sem_mensagens_novas',\n      palavras_delta: 0,\n      chat_id: item.id\n    }\n  }];\n}\n\n// Calcula palavras do DELTA (apenas do usuário)\nconst palavrasDelta = mensagensNovas\n  .filter(m => m.autor === 'usuario')\n  .reduce((sum, m) => sum + (m.texto || '').trim().split(/\\s+/).filter(w => w).length, 0);\n\n// Se delta < 30 palavras, não processar\nif (palavrasDelta < 30) {\n  return [{\n    json: {\n      skip: true,\n      motivo: 'delta_insuficiente',\n      palavras_delta: palavrasDelta,\n      chat_id: item.id\n    }\n  }];\n}\n\n// Formata apenas o delta de mensagens para enviar ao expert\nconst mensagensDelta = mensagensNovas.map(m => `${m.autor}: ${m.texto}`).join('\\n');\n\nreturn [{\n  json: {\n    skip: false,\n    usuario_id: item.usuario_id,\n    chat_id: item.id,\n    data_conversa: item.data_conversa,\n    mensagens_delta: mensagensDelta,\n    usr_nome_preferencia: item.usr_nome_preferencia || 'Usuário',\n    total_interactions: item.total_interactions,\n    ultima_processada: ultimaProcessada,\n    palavras_delta: palavrasDelta\n  }\n}];"
      },
      "id": "92a80786-2fcd-4d59-9d61-ffe1c42b65fe",
      "name": "prepara_dados",
      "type": "n8n-nodes-base.code",
