    {
      "parameters": {
        "operation": "executeQuery",
        "query": "-- Busca chats não finalizados com >= 30 palavras do usuário\nWITH palavras_calculadas AS (\n  SELECT \n    c.id,\n    c.usuario_id,\n    c.data_conversa,\n    c.mensagens,\n    c.total_interactions,\n    c.status,\n    u.nome_preferencia AS usr_nome_preferencia,\n    COALESCE(le.ultima_interaction, 0) AS ultima_processada,\n    -- Calcula palavras do usuário diretamente do JSON\n    COALESCE((\n      SELECT SUM(array_length(regexp_split_to_array(trim(msg->>'texto'), '\\s+'), 1))\n      FROM jsonb_array_elements(c.mensagens) AS msg\n      WHERE msg->>'autor' = 'usuario'\n    ), 0) AS palavras_usuario\n  FROM usr_chat c\n  JOIN usuarios u ON u.id = c.usuario_id\n  LEFT JOIN log_experts le ON le.chat_id = c.id AND le.expert_name = 'hub'\n  WHERE c.status != 'finalizado'\n    AND c.total_interactions > COALESCE(le.ultima_interaction, 0)\n)\nSELECT \n  id,\n  usuario_id,\n  data_conversa,\n  mensagens,\n  total_interactions,\n  palavras_usuario AS total_palavras,\n  usr_nome_preferencia,\n  ultima_processada\nFROM palavras_calculadas\nWHERE palavras_usuario >= 30\nORDER BY data_conversa DESC\nLIMIT 50;",
        "options": {}
      },
      "id": "6c8a1a58-8c9d-4f9a-9dc2-631bdc305bef",
      "name": "busca_validas",
