    {
      "parameters": {
        "operation": "executeQuery",
        "query": "-- Busca chats com interações NÃO processadas + dados completos\nSELECT \n  c.id,\n  c.usuario_id,\n  c.data_conversa,\n  c.mensagens,\n  c.total_interactions,\n  u.nome_preferencia AS usr_nome_preferencia,\n  COALESCE(le.ultima_interaction, 0) AS ultima_processada\nFROM usr_chat c\nJOIN usuarios u ON u.id = c.usuario_id\nLEFT JOIN log_experts le ON le.chat_id = c.id\nWHERE c.data_conversa >= CURRENT_DATE - INTERVAL '7 days'\n  AND c.total_interactions > COALESCE(le.ultima_interaction, 0)\nORDER BY c.data_conversa DESC\nLIMIT 50;",
        "options": {}
      },
      "id": "busca-pendentes-001",
      "name": "busca_pendentes",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.6,
