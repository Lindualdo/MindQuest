        }
      }
    },
    {
      "parameters": {
        "jsCode": "// PREPARA DADOS PARA OS EXPERTS\nconst item = $input.first().json;\n\nconst mensagens = item.mensagens || [];\n\n// Converte mensagens para texto\nconst mensagensTexto = Array.isArray(mensagens) \n  ? mensagens.map(m => `${m.autor}: ${m.texto}`).join('\\n')\n  : '';\n\nreturn [{\n  usuario_id: item.usuario_id,\n  chat_id: item.id,\n  data_conversa: item.data_conversa,\n  mensagens: mensagensTexto,\n  usr_nome_preferencia: item.usr_nome_preferencia || 'Usuário',\n  total_interactions: item.total_interactions\n}];"
      },
