    {
      "parameters": {
        "operation": "executeQuery",
        "query": "-- Upsert com histórico de execuções\nINSERT INTO log_experts (chat_id, ultima_interaction, historico, processado_em, atualizado_em)\nVALUES (\n  '{{ $json.chat_id }}'::uuid, \n  {{ $json.total_interactions }}, \n  jsonb_build_array(jsonb_build_object(\n    'de', 0,\n    'ate', {{ $json.total_interactions }},\n    'novas', {{ $json.total_interactions }},\n    'em', NOW()\n  )),\n  NOW(),\n  NOW()\n)\nON CONFLICT (chat_id) \nDO UPDATE SET \n  historico = log_experts.historico || jsonb_build_array(jsonb_build_object(\n    'de', log_experts.ultima_interaction,\n    'ate', {{ $json.total_interactions }},\n    'novas', {{ $json.total_interactions }} - log_experts.ultima_interaction,\n    'em', NOW()\n  )),\n  ultima_interaction = {{ $json.total_interactions }},\n  atualizado_em = NOW();",
        "options": {}
      },
      "id": "marca-processada-001",
      "name": "marca_processada",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.6,
      "position": [
        1184,
