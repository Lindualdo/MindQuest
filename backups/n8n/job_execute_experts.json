    {
      "parameters": {
        "operation": "executeQuery",
        "query": "SELECT c.id, c.usuario_id, c.data_conversa, c.status, c.mensagens,\n       u.usr_name, u.contexto_final\nFROM usr_chat c\nJOIN usuarios u ON u.id = c.usuario_id\nWHERE c.data_conversa = CURRENT_DATE\n  AND (c.processada_em IS NULL OR c.processada_em < c.atualizado_em)\n  AND c.mensagens IS NOT NULL\n  AND jsonb_array_length(c.mensagens) > 0\nORDER BY c.data_conversa DESC;",
        "options": {}
      },
