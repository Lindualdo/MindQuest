        1216,
        -192
      ]
    },
    {
      "parameters": {
        "operation": "executeQuery",
          "query": "-- Busca chats válidos: >= 30 palavras e com interações novas\n-- Calcula total_palavras diretamente do campo mensagens (autor = 'usuario')\nSELECT \n  c.id,\n  c.usuario_id,\n  c.data_conversa,\n  c.mensagens,\n  c.total_interactions,\n  (\n    SELECT COALESCE(SUM(array_length(string_to_array(m->>'texto', ' '), 1)), 0)\n    FROM jsonb_array_elements(c.mensagens) AS m\n    WHERE m->>'autor' = 'usuario'\n      AND m->>'texto' IS NOT NULL\n  ) AS total_palavras,\n  u.nome_preferencia AS usr_nome_preferencia,\n  COALESCE(le.ultima_interaction, 0) AS ultima_processada\nFROM usr_chat c\nJOIN usuarios u ON u.id = c.usuario_id\nLEFT JOIN log_experts le ON le.chat_id = c.id AND le.expert_name = 'hub'\nWHERE c.data_conversa >= CURRENT_DATE - INTERVAL '7 days'\n  AND c.total_interactions > COALESCE(le.ultima_interaction, 0)\n  AND (\n    SELECT COALESCE(SUM(array_length(string_to_array(m->>'texto', ' '), 1)), 0)\n    FROM jsonb_array_elements(c.mensagens) AS m\n    WHERE m->>'autor' = 'usuario'\n      AND m->>'texto' IS NOT NULL\n  ) >= 30\nORDER BY c.data_conversa DESC\nLIMIT 50;",
        "options": {}
      },
      "id": "6c8a1a58-8c9d-4f9a-9dc2-631bdc305bef",
      "name": "busca_validas",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.6,
      "position": [
        1424,
        -192
      ],
      "credentials": {
        "postgres": {
          "id": "8ySWxtSO7gYK5uue",
          "name": "Postgres account"
        }
      }
    },
    {
      "parameters": {
        "options": {}
      },
      "id": "a0159aa2-3292-4440-ae70-b51464131f70",
      "name": "Loop Over Items",
      "type": "n8n-nodes-base.splitInBatches",
      "typeVersion": 3,
      "position": [
        1664,
        -192
      ]
    },
    {
      "parameters": {
        "jsCode": "// Prepara dados para os experts\nconst item = $input.first().json;\nconst mensagens = item.mensagens || [];\nconst ultimaProcessada = item.ultima_processada || 0;\n\n// Extrai apenas mensagens novas (delta)\nconst mensagensNovas = Array.isArray(mensagens) ? mensagens.slice(ultimaProcessada) : [];\nconst mensagensDelta = mensagensNovas.map(m => `${m.autor}: ${m.texto}`).join('\\n');\n\nreturn [{\n  json: {\n    usuario_id: item.usuario_id,\n    chat_id: item.id,\n    data_conversa: item.data_conversa,\n    mensagens: mensagensDelta,\n    usr_nome_preferencia: item.usr_nome_preferencia || 'Usuário',\n    total_interactions: item.total_interactions,\n    ultima_processada: ultimaProcessada\n  }\n}];"
      },
      "id": "92a80786-2fcd-4d59-9d61-ffe1c42b65fe",
      "name": "prepara_dados",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        1872,
        -192
      ]
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "-- Upsert log + finaliza sessão em uma transação\nWITH upsert_log AS (\n  INSERT INTO log_experts (usuario_id, chat_id, expert_name, ultima_interaction, historico, processado_em, atualizado_em)\n  VALUES (\n    $1::uuid,\n    $2::uuid,\n    'hub',\n    $3::int,\n    jsonb_build_array(jsonb_build_object('de', 0, 'ate', $3::int, 'novas', $3::int, 'em', NOW())),\n    NOW(),\n    NOW()\n  )\n  ON CONFLICT (chat_id, expert_name) DO UPDATE SET\n    historico = log_experts.historico || jsonb_build_array(jsonb_build_object(\n      'de', log_experts.ultima_interaction,\n      'ate', $3::int,\n      'novas', $3::int - log_experts.ultima_interaction,\n      'em', NOW()\n    )),\n    ultima_interaction = $3::int,\n    atualizado_em = NOW()\n  RETURNING chat_id\n)\nUPDATE usr_chat SET\n  status = 'finalizado',\n  horario_fim = NOW(),\n  atualizado_em = NOW()\nWHERE id = $2::uuid\nRETURNING id, usuario_id, status;",
        "options": {
          "queryReplacement": "={{ [$json.usuario_id, $json.chat_id, $json.total_interactions] }}"
        }
      },
      "id": "d7569ce8-754c-4893-bad9-a152228c1f84",
      "name": "atualiza_log",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.6,
