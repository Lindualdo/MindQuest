    {
      "parameters": {
        "jsCode": "const entrada = $input.item.json || {};\nconst contexto = entrada.contexto || {};\n\nconst resumo = contexto.resumo_conversa || '';\nconst objetivos = contexto.objetivos_especificos || [];\n\nif (objetivos.length === 0) {\n  return [{\n    json: {\n      pode_criar: false,\n      motivo: 'sem_objetivos',\n      usuario_id: entrada.usuario_id\n    }\n  }];\n}\n\n// Usa resumo processado (sem validar tamanho - expert já validou)\nreturn [{\n  json: {\n    pode_criar: true,\n    usuario_id: entrada.usuario_id,\n    chat_id: entrada.chat_id,\n    objetivo_selecionado: objetivos[0],\n    resumo_conversa: resumo || 'conversa de reflexão',\n    contexto: contexto\n  }\n}];"
      },
      "id": "9ab944c3-2269-4ef8-bc79-9ddc7b7e8464",
